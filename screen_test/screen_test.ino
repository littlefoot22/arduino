void calculate_operation(byte first, byte second) {
    int8_t s_second = second;          
    switch (operator_id) {
      case 0:
        new_value = first + second;
        break;
      case 1:
        new_value = first - second;
        break;
      case 2:
        new_value = first / second;
        break;
      case 3:
        new_value = first * second;
        break;
      case 4:
        shift_values();
        break;
      case 5:
        frame_id = 5;
        break;
      case 6:
        boss_byte_0 = boss_byte_0 + s_second;
        frame_id = 7;
        break;
      case 7:
        boss_byte_0 = boss_byte_0 << 1;
        frame_id = 9;
        break;
      case 8:
        second = (second << 1) | (1 & 1);
        frame_id = 11;
        break;
    }
}

