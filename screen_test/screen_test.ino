    buttonState_8 = digitalRead(8);
    if (buttonState_8 == LOW) {
      mode = 'b';
    }

    buttonState_2 = digitalRead(2);
    if (buttonState_2 == LOW) {
      index = index + 1;
      if (index == 11) {
        index = 0;
      }
    }
  }

}

 
